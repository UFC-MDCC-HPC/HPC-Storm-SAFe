<?xml version="1.0" encoding="UTF-8"?>
<tns:architecture xmlns:tns="http://www.example.org/SAFe_architecture_V4"
	xmlns:tns1="http://www.example.org/instantiator" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/SAFe_architecture_V4 SAFe_architecture_V4.xsd ">

	<tns:application id="1" name="mImgtbl-simple">
		<tns:uses_port id="10" id_component="100"
			name="uses_images-rawdir-tbl" />
		<tns:provides_port id="11" id_component="300"
			name="provides_rawdir" />
		<tns:contract id="1">
			<tns:uri>http://tempuri.org-a</tns:uri>
		</tns:contract>
	</tns:application>

	<tns:workflow id="2" name="wf-mImgtbl">
		<tns:contract id="2">
			<tns:uri>http://tempuri.org-b</tns:uri>
		</tns:contract>
		<tns:task_port id="20">
			<tns:action id="201" name="run" />
		</tns:task_port>
	</tns:workflow>

	<tns:body>
		<tns:computation id="3" name="mImgtb">
			<tns:uses_port id="31" id_component="300" name="uses_rawdir" />
			<tns:provides_port id="32" id_component="100"
				name="provides_images-rawdir-tbl" />
			<tns:contract id="0">
				<tns:uri>http://tempuri.org.wf</tns:uri>
			</tns:contract>
			<tns:task_port id="33">
				<tns:action id="331" name="run"/>
			</tns:task_port>
		</tns:computation>
	</tns:body>

	<tns:env_binding>
		<tns:uses_port id="10" id_component="1"
			name="uses_images-rawdir-tbl" />
		<tns:provides_port id="32" id_component="3"
			name="provides_images-rawdir-tbl" />
	</tns:env_binding>
	<tns:env_binding>
		<tns:uses_port id="31" id_component="3" name="uses_rawdir" />
		<tns:provides_port id="11" id_component="1"
			name="provides_rawdir" />
	</tns:env_binding>

	<tns:task_binding>
		<tns:left_peer id="20">
			<tns:action id="201" name="run" />
		</tns:left_peer>
		<tns:right_peer id="33">
			<tns:action id="331" name="run" />
		</tns:right_peer>
	</tns:task_binding>

	<tns:parameter name="m101-dir" nature="IN" type="DIRECTORY" />
	<tns:parameter name="m101-tbl" nature="OUT" type="FILE" />

</tns:architecture>
