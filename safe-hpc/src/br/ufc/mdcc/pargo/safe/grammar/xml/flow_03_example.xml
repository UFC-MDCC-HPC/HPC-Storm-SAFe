<?xml version="1.0" encoding="UTF-8"?>
<tns:workflow xmlns:tns="http://www.example.org/SAFe_workflow_V3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/SAFe_workflow_V3 SAFe_workflow_V3.xsd ">
<tns:sequence_oper>

	<tns:operation>
		<tns:start_oper handle_id="0" >
			<tns:action action="compute" id="splitter.read_source"/>
		</tns:start_oper>
	</tns:operation>
	<tns:operation>
		<tns:start_oper handle_id="0" >
			<tns:action action="compute" id="splitter.perform"/>
		</tns:start_oper>
	</tns:operation>
	<tns:operation>
		 <tns:choice_oper>
		 	<tns:select_operation>
		 		<tns:operation>
		 			<tns:break_oper/>
		 		</tns:operation>
		 	</tns:select_operation>
		 </tns:choice_oper>
	</tns:operation>

</tns:sequence_oper>
</tns:workflow>
