	<tns:env_binding>
		<tns:uses id="10" />
		<tns:provides id="41" />
	</tns:env_binding>
	<tns:env_binding>
		<tns:uses id="11" />
		<tns:provides id="51" />
	</tns:env_binding>
	<tns:env_binding>
		<tns:uses id="12" />
		<tns:provides id="61" />
	</tns:env_binding>
	<tns:env_binding>
