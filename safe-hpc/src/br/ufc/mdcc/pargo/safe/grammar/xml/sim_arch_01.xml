				<tns:action id="232" name="wf_task_conn_232" />
			</tns:task_port>
		</tns:task_ports>
	</tns:workflow>

	<tns:body>
		<tns:connector name="conn" id="3">
			<tns:task_ports>
				<tns:task_port name="task_conn_wf" id="31">
					<tns:action id="311" name="conn_action_311" />
