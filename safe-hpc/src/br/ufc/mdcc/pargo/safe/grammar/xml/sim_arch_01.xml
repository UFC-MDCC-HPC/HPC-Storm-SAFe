			/>
		</tns:computation>

		<tns:computation name="comp_B" id="5">
			<tns:env_ports>
				<tns:provides name="B_prv_app" id="51" />
			</tns:env_ports>
			<tns:task_ports>
				<tns:task_port name="task_B_conn" id="52">
					<tns:action id="521" name="comp_B_action_521" />
