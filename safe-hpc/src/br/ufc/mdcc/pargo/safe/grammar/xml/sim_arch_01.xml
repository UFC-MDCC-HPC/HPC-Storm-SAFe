			</tns:task_ports>
			
			<tns:contract
				id="35"
				name="conn_contract"
				url="http://contract.com.br?conn.wsdl" 
			/>
			
		</tns:connector>

		<tns:computation name="comp_A" id="4">
			<tns:env_ports>
				<tns:provides name="A_prv_app" id="41" />
			</tns:env_ports>
			<tns:task_ports>
				<tns:task_port name="task_A_conn" id="42">
