	xsi:schemaLocation="http://www.example.org/SAFe_architecture_V2 SAFe_architecture_V2.xsd ">

	<tns:application name="app_component" id="1">
		<tns:env_ports>
			<tns:uses name="app_uses_A" id="10"/>
			<tns:uses name="app_uses_B" id="11" />
			<tns:uses name="app_uses_C" id="12" />
			<tns:uses name="app_uses_wf" id="13" />
			<tns:provides name="app_prv_wf" id="14" />
