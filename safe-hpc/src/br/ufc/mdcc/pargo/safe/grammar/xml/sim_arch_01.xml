		<tns:computation name="comp_C" id="6">
			<tns:env_ports>
				<tns:provides name="C_prv_app" id="61" />
			</tns:env_ports>
			<tns:task_ports>
				<tns:task_port name="task_C_conn" id="62">
					<tns:action id="621" name="comp_C_action_621" />
