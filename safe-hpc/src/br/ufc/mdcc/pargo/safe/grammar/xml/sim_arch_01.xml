	</tns:env_binding>
	<tns:env_binding>
		<tns:uses id="21" />
		<tns:provides id="14" />
	</tns:env_binding>

	<tns:task_binding>
		<tns:task_A id="23" />
		<tns:task_B id="31" />
	</tns:task_binding>
	<tns:task_binding>
		<tns:task_A id="32" />
		<tns:task_B id="42" />
	</tns:task_binding>
	<tns:task_binding>
		<tns:task_A id="33" />
