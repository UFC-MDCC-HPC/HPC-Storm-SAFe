<?xml version="1.0" encoding="UTF-8"?>
<!-- This is an example of workflow... -->
<tns:workflow xmlns:tns="http://www.example.org/SAFe_workflow_V4"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/SAFe_workflow_V4 SAFe_workflow_V4.xsd ">


	<tns:sequence_oper>
		<tns:operation>
			<tns:iterate_oper max="3">
				<tns:operation>
					<tns:start_oper handle_id="61">
						<tns:action action="compute" id="1" />
					</tns:start_oper>
				</tns:operation>
				<tns:operation>
					<tns:start_oper handle_id="62">
						<tns:action action="resolve" id="2" />
					</tns:start_oper>
				</tns:operation>
				<tns:operation>
					<tns:invoke_oper action="instantiate" id="2" />
				</tns:operation>
				
				<tns:operation>
					<tns:parallel_oper>
						<tns:operation>
							<tns:start_oper handle_id="100">
								<tns:action action="compute" id="1" />
							</tns:start_oper>
						</tns:operation>
						<tns:operation>
							<tns:sequence_oper>
								<tns:operation>
									<tns:cancel_oper>
										<tns:action action="resolve" id="454545"/>
									</tns:cancel_oper>
								</tns:operation>
								<tns:operation>
									<tns:skip_oper/>
								</tns:operation>
							</tns:sequence_oper>
						</tns:operation>
						<tns:operation>
							<tns:start_oper handle_id="102">
								<tns:action action="compute" id="1" />
							</tns:start_oper>
						</tns:operation>
					</tns:parallel_oper>
				</tns:operation>
				<tns:operation>
					<tns:start_oper handle_id="63">
						<tns:action action="compute" id="1" />
					</tns:start_oper>
				</tns:operation>
			</tns:iterate_oper>
		</tns:operation>
		
		
		
	</tns:sequence_oper>


</tns:workflow>