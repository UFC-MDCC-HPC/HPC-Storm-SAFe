<?xml version="1.0" encoding="UTF-8"?>
<tns:architecture xmlns:tns="http://www.example.org/SAFe_architecture_V4" 
xmlns:tns1="http://www.example.org/instantiator" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://www.example.org/SAFe_architecture_V4 SAFe_architecture_V4.xsd ">
  
   <!-- application -->
  <tns:application id="0" name="pleiades-C">
    <tns:uses_port id="13" name="port_SAFeSWL3" id_component="0"/>
    <tns:uses_port id="14" name="port_Go3" id_component="0"/>
  	<tns:provides_port id="12" name="tmpl-hdr-prov" id_component="0"/>
  </tns:application>
  
  <!-- workflow -->
  <tns:workflow id="1" name="wf-pleiades-C">
    <tns:provides_port id="21" name="port_SAFeSWL3" id_component="1"/>
    <tns:provides_port id="22" name="port_Go3" id_component="1"/>
    <tns:task_port id="23" name="wf-mimgtbl-tsk-C" id_component="1"/>
  	<tns:task_port id="24" name="wf-mprojexec-tsk-C" id_component="1"/>
  	<tns:task_port id="25" name="wf-madd-tsk-C" id_component="1"/>
  </tns:workflow>
 <tns:body>
  
  <!-- repository -->
  <tns:computation name="dss2rDir" id="200">
    <tns:provides_port id="2001" name="dss2r-proj" id_component="200"/>
    <tns:provides_port id="2002" name="dss2r-raw" id_component="200"/>
  </tns:computation>
  
  <!-- computation -->
    <tns:computation name="mImgtblC" id="2">
      <tns:uses_port id="31" name="mimgtbl-raw-dir-port-uses-C" id_component="2"/>
      <tns:uses_port id="32" name="mimgtbl-proj-dir-port-uses-C" id_component="2"/>
  	  <tns:provides_port id="33" name="mimgtbl-tbl-port-provides-C" id_component="2"/>
      <tns:task_port name="mimgtbl-go-C" id="34" id_component="2">
      		<tns:action id="341" name="go"/>	
      </tns:task_port>
    </tns:computation>
    
    <tns:computation name="mProjExecC" id="11">
      <tns:uses_port id="111" name="mprojexec-tbl-port-uses-C" id_component="11"/>
      <tns:uses_port id="112" name="mprojexec-dir-port-uses-in-C" id_component="11"/>
      <tns:uses_port id="113" name="mprojexec-dir-port-uses-out-C" id_component="11"/>
      <tns:uses_port id="114" name="mprojexec-hdr-port-uses-C" id_component="11"/>
      <tns:task_port name="mprojexec-go-C" id="115" id_component="11">
      		<tns:action id="1151" name="go"/>	
      </tns:task_port>
    </tns:computation>
    
    <tns:computation name="mAddC" id="4">
      <tns:uses_port id="41" name="madd-tbl-port-uses-C" id_component="4"/>
      <tns:uses_port id="42" name="madd-dir-port-uses-C" id_component="4"/>
      <tns:uses_port id="43" name="madd-hdr-port-uses-C" id_component="4"/>
      <tns:provides_port id="44" name="madd-fits-port-provides-C" id_component="4"/>
      <tns:task_port name="madd-go-C" id="45" id_component="4">
      		<tns:action id="451" name="go"/>	
      </tns:task_port>
    </tns:computation>
   
    
    
 </tns:body>
 
 <!-- Application bindings -->
  <tns:env_binding>
  	<tns:uses_port id="13" name="port_SAFeSWL3" id_component="0"/> 
  	<tns:provides_port id="21" name="port_SAFeSWL3" id_component="1"/>
  </tns:env_binding>
  <tns:env_binding>
  	<tns:uses_port id="14" name="port_Go3" id_component="0"/> 
  	<tns:provides_port id="22" name="port_Go3" id_component="1"/>
  </tns:env_binding>
  
  
    
  <!-- mImgtbl bindings -->
  <tns:env_binding>
  	<tns:uses_port id="31" name="mimgtbl-raw-dir-port-uses-C" id_component="2"/> 
  	<tns:provides_port id="2002" name="dss2r-raw" id_component="200"/>
  </tns:env_binding>
  <tns:env_binding>
  	<tns:uses_port id="32" name="mimgtbl-proj-dir-port-uses-C" id_component="2"/> 
  	<tns:provides_port id="2001" name="dss2r-proj" id_component="200"/>
  </tns:env_binding>
  
  <!-- mProjExec bindings -->
  <tns:env_binding>
  	<tns:uses_port id="111" name="mprojexec-tbl-port-uses-C" id_component="11"/> 
  	<tns:provides_port id="33" name="mimgtbl-tbl-port-provides-C" id_component="2"/>
  </tns:env_binding>
  <tns:env_binding>
  	<tns:uses_port id="112" name="mprojexec-dir-port-uses-in-C" id_component="11"/> 
  	<tns:provides_port id="2002" name="dss2r-raw" id_component="200"/>
  </tns:env_binding>
  <tns:env_binding>
  	<tns:uses_port id="113" name="mprojexec-dir-port-uses-out-C" id_component="11"/> 
  	<tns:provides_port id="2001" name="dss2r-proj" id_component="200"/>
  </tns:env_binding>
  <tns:env_binding>
  	<tns:uses_port id="114" name="mprojexec-hdr-port-uses-C" id_component="11"/> 
  	<tns:provides_port id="12" name="tmpl-hdr-prov" id_component="0"/>
  </tns:env_binding>
  
  <!-- mAdd bindings -->
  <tns:env_binding>
  	<tns:uses_port id="41" name="madd-tbl-port-uses-C" id_component="4"/> 
  	<tns:provides_port id="33" name="mimgtbl-tbl-port-provides-C" id_component="2"/>
  </tns:env_binding>
  <tns:env_binding>
  	<tns:uses_port id="42" name="madd-dir-port-uses-C" id_component="4"/> 
  	<tns:provides_port id="2001" name="dss2r-proj" id_component="200"/>
  </tns:env_binding>
  <tns:env_binding>
  	<tns:uses_port id="43" name="madd-hdr-port-uses-C" id_component="4"/> 
  	<tns:provides_port id="12" name="tmpl-hdr-prov" id_component="0"/>
  </tns:env_binding>
  
  
  
  
  <!-- TASK bindings -->
  <tns:task_binding>
  	<tns:left_peer id="23" name="wf-mimgtbl-tsk-C" id_component="1"/>
  	<tns:right_peer name="mimgtbl-go-C" id="34" id_component="2"/>
  </tns:task_binding>
  <tns:task_binding>
  	<tns:left_peer id="24" name="wf-mprojexec-tsk-C" id_component="1"/>
  	<tns:right_peer name="mprojexec-go-C" id="115" id_component="11"/>
  </tns:task_binding>
  <tns:task_binding>
  	<tns:left_peer id="25" name="wf-madd-tsk-C" id_component="1"/>
  	<tns:right_peer name="madd-go-C" id="45" id_component="4"/>
  </tns:task_binding>
  
  
</tns:architecture>
