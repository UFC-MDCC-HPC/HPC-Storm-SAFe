<?xml version="1.0" encoding="UTF-8"?>
<tns:workflow xmlns:tns="http://www.example.org/SAFe_workflow_V4"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/SAFe_workflow_V4 SAFe_workflow_V4.xsd ">


	<tns:sequence_oper>
		<tns:operation>
			<tns:parallel_oper>
				
				<!-- client -->
				<tns:operation>
					<tns:sequence_oper>
						<tns:operation>
							<tns:invoke_oper action="resolve" id="2" />
						</tns:operation>
						<tns:operation>
							<tns:invoke_oper action="deploy" id="2" />
						</tns:operation>
						<tns:operation>
							<tns:invoke_oper action="instantiate" id="2" />
						</tns:operation>
					</tns:sequence_oper>
				</tns:operation>
			
				<!-- server -->
				<tns:operation>
					<tns:sequence_oper>
						<tns:operation>
							<tns:invoke_oper action="resolve" id="3" />
						</tns:operation>
						<tns:operation>
							<tns:invoke_oper action="deploy" id="3" />
						</tns:operation>
						<tns:operation>
							<tns:invoke_oper action="instantiate" id="3" />
						</tns:operation>
					</tns:sequence_oper>
				</tns:operation>
				
				<!-- queue -->
				<tns:operation>
					<tns:sequence_oper>
						<tns:operation>
							<tns:invoke_oper action="resolve" id="4" />
						</tns:operation>
						<tns:operation>
							<tns:invoke_oper action="deploy" id="4" />
						</tns:operation>
						<tns:operation>
							<tns:invoke_oper action="instantiate" id="4" />
						</tns:operation>
					</tns:sequence_oper>
				</tns:operation>
				
			</tns:parallel_oper>
		</tns:operation>
		
		<!-- connect application uses port -->
		<tns:operation>
			<tns:invoke_oper action="connect" id="0" />
		</tns:operation>
		
		<!-- post -->
		<tns:operation>
			<tns:invoke_oper action="compute" id="321" />
		</tns:operation>
		
		<!-- send -->
		<tns:operation>
			<tns:invoke_oper action="compute" id="322" />
		</tns:operation>
		
		<!-- go -->
		<tns:operation>
			<tns:invoke_oper action="compute" id="511" />
		</tns:operation>
		
		<!-- get -->
		<tns:operation>
			<tns:invoke_oper action="compute" id="411" />
		</tns:operation>
		
		<!-- request -->
		<tns:operation>
			<tns:invoke_oper action="compute" id="412" />
		</tns:operation>
		
	</tns:sequence_oper>
</tns:workflow>
